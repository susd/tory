
<div class="row">
  <div class="small-12 columns">
    <h3>Users</h3>
    <p><%= link_to 'Add user', new_admin_user_path %></p>
    <table>
      <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td><td><%= link_to 'Edit', edit_admin_user_path(user) %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>